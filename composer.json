{
    "name": "pierlo/wp-lazy-load-admin",
    "description": "Lazy load for the WordPress admin panel",
    "type": "project",
    "authors": [
        {
            "name": "Pierre Gordon",
            "email": "pierregordon@protonmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^2.1",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "10up/wp_mock": "^0.4.2",
        "mockery/mockery": "^1.2",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Pierlo\\AdminLazyLoad\\": "php/"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Pierlo\\AdminLazyLoadTest\\": "tests/php/"
		}
	}
}
